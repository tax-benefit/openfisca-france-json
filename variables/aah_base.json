      "file_path": "openfisca_france/model/prestations/minima_sociaux/aah.py",
      "parameters": [
        "prestations.minima_sociaux.aah.montant"
      ],
      "source_code": "def formula(individu, period, parameters):\n    law = parameters(period).prestations\n\n    aah_eligible = individu('aah_eligible', period)\n    aah_base_ressources = individu.famille('aah_base_ressources', period) / 12\n    plaf_ress_aah = individu('aah_plafond_ressources', period)\n    # Le montant de l'AAH est plafonné au montant de base.\n    montant_max = law.minima_sociaux.aah.montant\n    montant_aah = min_(montant_max, max_(0, plaf_ress_aah - aah_base_ressources))\n\n    aah_base_non_cumulable = individu('aah_base_non_cumulable', period)\n\n    return aah_eligible * min_(montant_aah, max_(0, montant_max - aah_base_non_cumulable))\n",
      "start_line_number": 315,
      "stop_line_number": 328,
      "variables": [
        "aah_base_non_cumulable",
        "aah_base_ressources",
        "aah_eligible",
        "aah_plafond_ressources"
      ]
    }
